<div class="main-content bg-white h-100">
<div class="header bg-primary pb-6">
      <div class="container-fluid">
        <div class="header-body">
          <div class="row align-items-center">
            <div class="col-lg-6 col-7">
              <h6 class="h2 text-white d-inline-block mb-0">Add Entity</h6>
              <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
                <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
                  <li class="breadcrumb-item"><a href="<%= authenticated_root_path %>"><i class="fas fa-home"></i></a></li>
                  <li class="breadcrumb-item">
                    <%= link_to 'Entities', entities_path %>
                  </li>
                  <li class="breadcrumb-item active" aria-current="page">New</li>
                </ol>
              </nav>
            </div>
          </div>
        </div>
    </div>
</div>

<div class="container pb-5 text-gray">
  <div class="row justify-content-center">
    <div class="col-lg-5 col-md-7">
      <div class="card bg-secondary border border-soft mb-0 p-5">
        <div class="text-center mb-4">
          <small>Enter the details of your organization</small>
        </div>
        <%= render 'form' %>
      </div>
    </div>
  </div>
</div>
</div>
